<h1>Tests</h1>

<p>
  <%= link_to 'New_test', new_test_path %>
</p>

<table>
  <tr>
    <th>Title</th>
    <th>Level</th>
    <th colspan="3">Actions</th>
  </tr>

  <%= @tests.each do |test| %>
    <tr>
      <td><%=test.title %></td>
      <td><%=test.level %></td>
    </tr>
  <% end %>
</table>
<%= render 'questions/index'
